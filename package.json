{
  "name": "power-up-magazine",
  "version": "0.6.0",
  "description": "A great way to learn about nuclear power, the cleanest, most reliable source of energy.",
  "productName": "Power Up Magazine",
  "author": "Happy Spirit Publishing",
  "license": "ISC",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "build": "quasar build",
    "start": "node server.js",
    "lint": "eslint --ext .js,.vue ./",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "test:jest": "quasar test --unit jest",
    "mockapi": "json-server --watch src/store/db.json",
    "test:unit": "jest --updateSnapshot",
    "test:unit:ci": "jest --ci",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.14.0",
    "axios": "^0.27.0",
    "connect-history-api-fallback": "^1.6.0",
    "core-js": "^3.23.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "global": "^4.4.0",
    "jwt-decode": "^3.1.2",
    "pinia": "^2.0.11",
    "quasar": "^2.7.0",
    "serve-static": "^1.14.1",
    "vue": "^3.2.0",
    "vue-axios-hooks": "^0.1.6",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/eslint-parser": "^7.17.0",
    "@quasar/app": "^3.3.0",
    "@quasar/cli": "^1.3.0",
    "@quasar/quasar-app-extension-testing": "^2.0.0",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
    "dotenv": "^16.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^26.5.0",
    "eslint-plugin-vue": "^9.1.0",
    "eslint-webpack-plugin": "^3.2.0"
  },
  "browserslist": [
